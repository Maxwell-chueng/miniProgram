page {
  background-color: #dedede;
  padding: 20rpx;
}
page .searchPage .topBar {
  height: 60rpx;
  display: flex;
}
page .searchPage .topBar input {
  background-color: #fff;
  flex: 1;
  height: 100%;
  border-radius: 5rpx;
  padding-left: 25rpx;
}
page .searchPage .topBar .btn {
  width: 110rpx;
  height: 100%;
  padding: 0;
  background-color: #fff;
  border-radius: 5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
}
page .searchPage .searchContent {
  margin-top: 30rpx;
}
page .searchPage .searchContent .items {
  font-size: 32rpx;
  background-color: #fff;
  margin-top: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
